{
  "verbose": true,
  "preset": "ts-jest",
  "testEnvironment": "node",
  "setupFilesAfterEnv": ["./src/test/setup.ts"],
  "collectCoverageFrom": ["src/**/*.ts"],
  "testPathIgnorePatterns": ["build", "src/example"],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "coveragePathIgnorePatterns": ["build", "src/example"]
}
